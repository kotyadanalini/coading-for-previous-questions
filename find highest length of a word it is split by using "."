import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
       String s=sc.nextLine();
       String st[]=s.split("\\.");
       int n=0;
       for(int i=0;i<st.length;i++){
           if(st[i].length()>n){
               n=st[i].length();
           }
       }
       System.out.println(n);
      
    }
}
